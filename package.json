{
  "name": "directivue",
  "version": "0.1.2",
  "description": "Lightweight and Optimized Vue Directives for your Vue projects",
  "repository": "kevinmarrec/directivue",
  "keywords": [
    "vue",
    "vue.js",
    "vuejs",
    "vue directive",
    "directive"
  ],
  "contributors": [
    {
      "name": "Kevin Marrec (@kevinmarrec)"
    },
    {
      "name": "Erwan Jugand (@erwanjugand)"
    }
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && tsc && copyfiles -u 1 src/**/*.css dist",
    "clean": "rimraf dist",
    "demo:generate": "yarn build && cd demo && yarn generate",
    "postinstall": "cd demo && yarn"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "latest",
    "copyfiles": "latest",
    "eslint": "latest",
    "typescript": "latest",
    "vue": "2"
  }
}
